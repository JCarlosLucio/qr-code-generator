{
  "name": "qr-code-generator",
  "version": "0.0.1",
  "private": true,
  "description": "Simple Qr Generator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCarlosLucio/qr-code-generator.git"
  },
  "keywords": [
    "qr",
    "generator"
  ],
  "author": "Juan Carlos Lucio",
  "bugs": {
    "url": "https://github.com/JCarlosLucio/qr-code-generator/issues"
  },
  "homepage": "https://github.com/JCarlosLucio/qr-code-generator#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md}\" ",
    "lint": "npm run lint:format && npm run lint:fix ",
    "type-check": "tsc",
    "cypress:open": "cypress open",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "daisyui": "^2.42.1",
    "qrcode": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.14.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "postcss": "^8.4.24",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "pre-commit": "lint",
  "license": "ISC"
}